<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff99dd"
    >

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="172dp"
        android:layout_height="364dp"
        android:contentDescription="@string/photo1"
        android:src="@drawable/q1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="-50dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="172dp"
        android:layout_height="364dp"
        android:contentDescription="@string/photo1"
        android:src="@drawable/q7"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="222dp"
        tools:layout_editor_absoluteY="204dp" />

    <ImageView
        android:layout_width="198dp"
        android:layout_height="360dp"
        android:contentDescription="@string/photo2"
        android:src="@drawable/q2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="343dp" />

    <Button
        android:id="@+id/who_are_we"
        android:layout_width="173dp"

        android:layout_height="55dp"
        android:layout_marginTop="59dp"
        android:background="@color/white"
        android:text="@string/string2"
        android:textColor="@color/black"

        android:textSize="19sp"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="222dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="168dp"
        android:layout_height="240dp"
        android:contentDescription="@string/photo3"
        android:src="@drawable/q3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="227dp"
        tools:layout_editor_absoluteY="403dp" />

    <ImageView
        android:layout_width="198dp"
        android:layout_height="260dp"
        android:contentDescription="@string/photo3"
        android:src="@drawable/q9"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="247dp" />

    <ImageView
        android:layout_width="198dp"
        android:layout_height="273dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:contentDescription="@string/photo2"
        android:src="@drawable/q4"
        app:layout_constraintEnd_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>